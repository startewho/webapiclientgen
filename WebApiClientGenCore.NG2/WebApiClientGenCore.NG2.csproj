<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <PackageId>Fonlow.WebApiClientGenCore.NG2</PackageId>
    <Authors>Zijian Huang</Authors>
    <Description>Plugin for WebApiClientGen 3.0+ to generate client API codes with Angular 6+.</Description>
    <PackageReleaseNotes>Doc comments for generic parameters; option HelpStrictMode for giving potential null signals to TypeScript strict mode.</PackageReleaseNotes>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageProjectUrl>https://github.com/zijianhuang/webapiclientgen</PackageProjectUrl>
    <Copyright>Copyright Â© Zijian Huang 2015-2022</Copyright>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <AssemblyName>Fonlow.WebApiClientGenCore.NG2</AssemblyName>
    <Version>3.1</Version>
    <PackageTags>Angular</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Fonlow.DocCommentCore\Fonlow.DocCommentCore.csproj" />
    <ProjectReference Include="..\Fonlow.Poco2TsCore\Fonlow.Poco2TsCore.csproj" />
    <ProjectReference Include="..\Fonlow.TypeScriptCodeDomCore\Fonlow.TypeScriptCodeDomCore.csproj" />
    <ProjectReference Include="..\Fonlow.Web.MetaCore\Fonlow.Web.MetaCore.csproj" />
    <ProjectReference Include="..\WebApiClientGenCore.Abstract\WebApiClientGenCore.Abstract.csproj" />
  </ItemGroup>

</Project>
